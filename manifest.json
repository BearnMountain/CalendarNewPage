{
	"manifest_version": 2,
	"name": "Calendar Startup",
	"version": "1.0",
	"description": "A custom calendar page that stores events.",
	"permissions": ["storage"],
	"background": {
		"scripts": ["background.js"]
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"web_accessible_resources": [
		"scripts/*"
	]
}
